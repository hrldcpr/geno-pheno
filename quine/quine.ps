% setup
400 720 moveto
10 10 scale
/Courier findfont
1 scalefont
setfont

% body
[
  (] dup)
  (-1 1 scale)
  (/p {)
  (  gsave { show } forall grestore)
  (  0 -1 rmoveto)
  (} def)
  ([([)] p)
  ({ /x exch def [(  \() x (\))] p } forall)
  ({ /x exch def [x] p } forall)
] dup
-1 1 scale
/p {
  gsave { show } forall grestore
  0 -1 rmoveto
} def
[([)] p
{ /x exch def [(  \() x (\))] p } forall
{ /x exch def [x] p } forall
